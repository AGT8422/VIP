{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "aloha/twilio": "4.0.4",
        "anakadote/image-manager": "^1.1",
        "anhskohbo/no-captcha": "^3.6",
        "arcanedev/log-viewer": "*",
        "automattic/woocommerce": "*",
        "balping/json-raw-encoder": "^1.0",
        "barryvdh/laravel-dompdf": "*",
        "barryvdh/laravel-snappy": "^1.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "composer/semver": "*",
        "consoletvs/charts": "^6.7",
        "doctrine/dbal": "^3.5",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^6.8",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "*",
        "intervention/image": "^2.7",
        "khaled.alshamaa/ar-php": "^6.3",
        "knox/pesapal": "*",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^1.6",
        "laravel/passport": "^10.1",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "laravelcollective/html": "*",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "*",
        "milon/barcode": "*",
        "mpdf/mpdf": "^8.2",
        "mpociot/laravel-apidoc-generator": "^4.8",
        "nexmo/laravel": "*",
        "nwidart/laravel-menus": "*",
        "nwidart/laravel-modules": "8.*",
        "php-open-source-saver/jwt-auth": "^2.1",
        "phpmailer/phpmailer": "^6.8",
        "pusher/pusher-php-server": "^7.0",
        "ramsey/uuid": "^4.1",
        "razorpay/razorpay": "*",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/flysystem-dropbox": "*",
        "spatie/laravel-activitylog": "*",
        "spatie/laravel-backup": "*",
        "spatie/laravel-permission": "*",
        "srmklive/paypal": "*",
        "stripe/stripe-php": "^13.6",
        "symfony/mailer": "^6.1",
        "swiftmailer/swiftmailer": "^6.0",
        "tecnickcom/tcpdf": "^6.6",
        "unicodeveloper/laravel-paystack": "*",
        "yajra/laravel-datatables-oracle": "v9.15.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^3.1",
        "ladumor/laravel-pwa": "^0.0.4",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
      "files": [
            "app/Http/helpers.php"
        ],
        "classmap": [
            "database",
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Classbased\\": "database/classbased/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }


}
